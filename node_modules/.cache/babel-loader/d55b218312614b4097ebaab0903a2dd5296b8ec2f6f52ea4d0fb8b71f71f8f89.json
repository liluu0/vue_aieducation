TemplateList()\r\n        console.log(res.data.data.data);\r\n        this.pptList = res.data.data.data\r\n      } catch (error) {\r\n        console.log('reqPptTemplateList',error);\r\n      }\r\n    },\r\n    pptItem(){\r\n      console.log('pptItem');\r\n    }\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.ppt-list-item .img-box .use-template {\r\n    width: 140px;\r\n    display: none;\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%,-50%);\r\n    line-height: 20px;\r\n    border-radius: 10px;\r\n    background: #89a7c8;\r\n    color: #fff;\r\n    border: 0;\r\n    cursor: pointer;\r\n}\r\n.py-7px {\r\n    padding-top: 7px;\r\n    padding-bottom: 7px;\r\n}\r\n.ppt-list-item .img-box:before {\r\n    content: \"\";\r\n    position: absolute;\r\n    top: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    background: rgba(0,0,0,.32);\r\n    opacity: 0;\r\n    transition: all .3s ease;\r\n    border-radius: 12px;\r\n}\r\n.ppt-list-item .list-item-content {\r\n    padding: 16px 12px 12px;\r\n}\r\n.ppt-list-item .img-box {\r\n    position: relative;\r\n    height: 160px;\r\n    border: 1px solid #e3deef;\r\n    border-radius: 12px;\r\n    background-size: 100% 100%;\r\n    background-color: #fff;\r\n}\r\n.ppt-list-item {\r\n    width: calc(25% - 32px);\r\n}\r\n\r\n.ppt-list-item {\r\n    width: 240px;\r\n    flex-shrink: 0;\r\n    margin: 15px;\r\n    cursor: pointer;\r\n    border-radius: 12px;\r\n    background: rgba(255,255,255,.72);\r\n    overflow: hidden;\r\n    transition: transform 0.05s;\r\n}\r\n\r\n.ppt-list-item:hover {\r\n    transform: scale(1.02); /* 鼠标移入时放大图片 */\r\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.3); /* 添加阴影效果 */\r\n}\r\n.ppt-list-item:hover .use-template{\r\n    display: block;\r\n}\r\n.template-list-wrap {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n}\r\n/* 模块title */\r\n.pt-28px {\r\n    padding-top: 28px;\r\n}\r\n.pb-8px {\r\n    padding-bottom: 8px;\r\n}\r\n.px-20px {\r\n    padding-left: 20px;\r\n    padding-right: 20px;\r\n}\r\n.text-16px {\r\n    font-size: 16px;\r\n    line-height: 1;\r\n}\r\n.font-700, .font-bold {\r\n    font-weight: 700;\r\n}\r\n</style>"],"mappings":"AA6BA,SAAQA,kBAAkB,QAAO,OAAM;AACvC,eAAe;EACbC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,OAAO,EAAC;IACV;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC;EACnB,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMD,WAAWA,CAAA,EAAE;MACjB,IAAI;QACF,MAAME,GAAE,GAAI,MAAMN,kBAAkB,CAAC;QACrCO,OAAO,CAACC,GAAG,CAACF,GAAG,CAACL,IAAI,CAACA,IAAI,CAACA,IAAI,CAAC;QAC/B,IAAI,CAACC,OAAM,GAAII,GAAG,CAACL,IAAI,CAACA,IAAI,CAACA,IAAG;MAClC,EAAE,OAAOQ,KAAK,EAAE;QACdF,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAACC,KAAK,CAAC;MACzC;IACF,CAAC;IACDC,OAAOA,CAAA,EAAE;MACPH,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;IACxB;EACF;AAEF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}