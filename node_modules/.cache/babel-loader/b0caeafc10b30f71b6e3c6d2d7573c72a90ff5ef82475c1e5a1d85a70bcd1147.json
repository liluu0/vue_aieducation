r\n                                \"label\": 3704,\r\n                                \"type\": \"用品\",\r\n                                \"english\": \"indian-red\",\r\n                                \"chinese\": \"印红\"\r\n                            },\r\n                            {\r\n                                \"label\": 16941,\r\n                                \"type\": \"用品\",\r\n                                \"english\": \"ball-gown\",\r\n                                \"chinese\": \"舞会礼服\"\r\n                            }\r\n                        ]\r\n              }]\r\n          }\r\n      },\r\n      methods:{\r\n      getCurrentTime() {\r\n        return new Date().toLocaleString();\r\n      },\r\n      imageBtn(){\r\n        // console.log('上传图片');\r\n        this.$refs.fileInput.click();\r\n      },\r\n      async handleFileUpload(event) {\r\n        const file1 = event.target.files[0];\r\n        // 创建 FormData 对象\r\n        let file = new FormData();\r\n        file.append(\"file\", file1);\r\n        let seeImage = ''\r\n        try {\r\n          const res = await reqPictureUpload(file)\r\n          seeImage = res.data.data\r\n          const time = this.getCurrentTime()\r\n            this.chatData.push({\r\n                chatTime: time,\r\n                chatUser: seeImage,\r\n                chatGpt: []\r\n             });\r\n            const lastItem = this.chatData[this.chatData.length - 1];\r\n            lastItem.chatGpt = [\r\n                            {\r\n                                \"label\": 16946,\r\n                                \"type\": \"用品\",\r\n                                \"english\": \"balldress\",\r\n                                \"chinese\": \"晚装\"\r\n                            }]\r\n\r\n        } catch (error) {\r\n            console.error('reqPictureUpload',error);\r\n        }\r\n\r\n      },\r\n\r\n\r\n      },\r\n    \r\n  }\r\n</script>\r\n  \r\n<style scoped>\r\n  .introduceleft {\r\n