essage.error('请输入正确的手机号');\n      } else if (this.signupData.password != this.signupData.password_again) {\n        Message.error('两次密码不一致');\n      } else {\n        console.log('注册');\n        try {\n          const response = await reqSignup(this.signupData);\n          // 请求成功处理逻辑\n          console.log(response.data.data);\n          // this.gridData = response.data.data\n        } catch (error) {\n          Message.error('注册失败');\n          console.error('Error fetching data:', error);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["reqLogin","reqSignup","Message","data","loginData","phone","password","userType","signupData","password_again","mounted","siginBtn","document","getElementById","sigupBtn","siginForm","querySelector","sigupForm","siginImgWrapper","sigupImgWrapper","animations","querySelectorAll","nav","form","center","flag","onclick","style","top","transform","classList","add","remove","setTimeout","zIndex","i","length","transition","opacity","methods","loginBtn","error","console","log","signUpBtn","regexp_1","str","test","response"],"sources":["D:\\代码\\vscodeProjects\\AiEducation\\vue_aieducation\\src\\components\\Login\\index.vue"],"sourcesContent":["<template>\r\n   <div class=\"content\">\r\n        <div class=\"content-wrapper\">\r\n            <div class=\"nav\">\r\n                <div class=\"logo\">\r\n                    <img src=\"./asset/logo.jpg\" alt=\"\">\r\n                </div>\r\n                <div class=\"nav-item\" id=\"nav-sigin-btn\">\r\n                    <img src=\"./asset/sigin.png\" alt=\"\">\r\n                    <span>Login</span>\r\n                    <div class=\"block\"></div>\r\n                </div>\r\n                <div class=\"nav-item\" id=\"nav-sigup-btn\">\r\n                    <img src=\"./asset/signup.png\" alt=\"\">\r\n                    <span>SigUp</span>\r\n                    <div class=\"block\"></div>\r\n                </div>\r\n            </div>\r\n            <div class=\"center\">\r\n                <div class=\"sigin-img-wrapper\">\r\n                    <div class=\"img-tips\">\r\n                        <h1>Start Your Work</h1>\r\n                        <h5>One Key adn there links</h5>\r\n                    </div>\r\n                </div>\r\n                <div class=\"sigup-img-wrapper\">\r\n                    <div class=\"img-tips\">\r\n                        <h1>Start Your Work</h1>\r\n                        <h5>One Key adn there links</h5>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <!-- 表单 -->\r\n            <div class=\"form\">\r\n                <div class=\"form-wrapper\">\r\n                    <div class=\"sigin common-style\">\r\n                        <div class=\"title\">\r\n                            <span>Don't have an account?</span>\r\n                            <span>Sign Up.</span>\r\n                        </div>\r\n                        <div class=\"inputs-wrapper\">\r\n                            <div class=\"inputs-item items-email\">\r\n                                <span class=\"input-tips\">Phone</span>\r\n                                <input type=\"text\" class=\"inputs\" v-model=\"loginData.phone\">\r\n                            </div>\r\n                            <div class=\"inputs-item items-pwd\">\r\n                                <span class=\"input-tips\">Password</span>\r\n                                <input type=\"password\" class=\"inputs\" v-model=\"loginData.password\">\r\n                            </div>\r\n                            <div class=\"inputs-item\">\r\n                                <input class=\"radio\" type=\"radio\" name=\"options\" value=\"0\" v-model=\"loginData.userType\">\r\n                                <label class=\"input-tips\" for=\"option1\">学生</label>\r\n\r\n                                <input class=\"radio\" type=\"radio\" name=\"options\" value=\"1\" v-model=\"loginData.userType\"